/// Put the interfaces in a module, to avoid global namespace pollution
module Test
{
	union UnionEnum switch (long)
	{
		case 1: short enum_short;
		case 2: string enum_string;
		case 3: long enum_long;
		
	};

	struct Message{
		long time;
		string inst_name;
		long long_bois[25];
		UnionEnum test;
		//sequence<long, 50> long_bois; 
	};

	/// A very simple interface
	interface Hello
	{
		exception TestException{
			string message;
		};

		void send(in Test::Message message);
		void send22(in Test::Message message) raises(TestException);
	};
};
